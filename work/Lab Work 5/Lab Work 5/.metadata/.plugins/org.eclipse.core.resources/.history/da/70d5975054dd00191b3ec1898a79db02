package Template;

public class Main {
	public static void main(String[] args) {
		ScrambledEggs scrambled = new ScrambledEggs();
		scrambled.crackEggs(2);
		scrambled.stirEggs();
		scrambled.cooking();
		scrambled.serve();
		System.out.println("---------------------------------");
		Omelette omelette = new Omelette();
		omelette.crackingEggs(3);
		omelette.prepareEggs();
		omelette.cook();
		omelette.serve();
		System.out.println("---------------------------------");
		SunnySide sunny = new SunnySide();
		sunny.crackEggs(1);
		sunny.prepare();
		sunny.cook();
		sunny.serve();
	}

}
